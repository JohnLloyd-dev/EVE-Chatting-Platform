# EVE Chatting Platform Environment Configuration
# Copy this file to .env and update the values for your deployment

# Database Configuration
POSTGRES_USER=adam2025man
POSTGRES_PASSWORD=adam2025
POSTGRES_DB=chatting_platform
DATABASE_URL=postgresql://adam2025man:adam2025@postgres:5432/chatting_platform

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# JWT Configuration
SECRET_KEY=YourSuperSecretJWTKey789!@#$%^&*()_+-=[]{}|;:,.<>?
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Admin Configuration
ADMIN_USERNAME=admin
ADMIN_PASSWORD=YourSecureAdminPassword789!

# Tally Webhook Security
TALLY_WEBHOOK_SECRET=YourTallyWebhookSecret123!

# AI Model Configuration (GGUF format)
AI_MODEL_NAME=TheBloke/OpenHermes-2.5-Mistral-7B-GGUF
AI_MODEL_FILE=openhermes-2.5-mistral-7b.Q5_K_M.gguf
AI_MODEL_CACHE_DIR=/app/.cache/huggingface
AI_GENERATION_TIMEOUT=30.0
AI_REQUEST_TIMEOUT=60.0

# GGUF Model Settings (optimized for RTX 4060 with 8GB VRAM)
AI_USE_GGUF=true
AI_GGUF_N_GPU_LAYERS=35
AI_GGUF_N_CTX=2048
AI_GGUF_N_BATCH=512

# Legacy Transformers Settings (fallback)
AI_USE_4BIT=false
AI_USE_8BIT=false

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8001

# Security Settings
ENVIRONMENT=development
DEBUG=true 